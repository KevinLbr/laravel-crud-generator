<?php

namespace App\Models;

use App\Traits\BelongsToMedia;
use KevinLbr\CrudGenerator\Traits\FormGeneration;
use Illuminate\Database\Eloquent\Model;

class DummyClass extends Model
{
    use FormGeneration, BelongsToMedia;

    protected $fillable = [fillables];

    public function getImg($width = null, $height= null)
    {
        return $this->media()->exists() ? $this->media->maxWidthRoute($width) : '/images/img-default.jpeg';
    }
}
